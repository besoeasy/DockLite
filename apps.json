{
  "apps": [
    {
      "name": "metube",
      "description": "Web-based YouTube downloader with a simple interface",
      "homepage": "https://github.com/alexta69/metube",
      "category": "media",
      "command": "docker run -d -p 4000:8081 --restart unless-stopped ghcr.io/alexta69/metube"
    },
    {
      "name": "speedtest",
      "description": "Self-hosted internet speed test server",
      "homepage": "https://github.com/librespeed/speedtest",
      "category": "networking",
      "command": "docker run -d -p 4001:8080 --restart unless-stopped ghcr.io/librespeed/speedtest"
    },
    {
      "name": "webcheck",
      "description": "Tool for analyzing website performance and security",
      "homepage": "https://github.com/lissy93/web-check",
      "category": "monitoring",
      "command": "docker run -d -p 4002:3000 --restart on-failure lissy93/web-check"
    },
    {
      "name": "morphos",
      "description": "Self-hosted server for private file format conversion (images for now, documents coming soon)",
      "homepage": "https://github.com/danvergara/morphos",
      "category": "utilities",
      "command": "docker run -d -p 4003:8080 --restart unless-stopped ghcr.io/danvergara/morphos-server:latest"
    },
    {
      "name": "filedrop",
      "description": "Simple file sharing and transfer service",
      "homepage": "https://github.com/besoeasy/file-drop",
      "category": "filesharing",
      "command": "docker run -d -p 4004:3232 --restart unless-stopped ghcr.io/besoeasy/file-drop:main"
    },
    {
      "name": "specterdesktop",
      "description": "A desktop GUI for Bitcoin Core optimised to work with hardware wallets",
      "homepage": "https://github.com/cryptoadvance/specter-desktop",
      "category": "cryptocurrency",
      "command": "docker run -d -p 4006:25441 --restart unless-stopped ghcr.io/cryptoadvance/specter-desktop:v2.1.1"
    },
    {
      "name": "uptimekuma",
      "description": "A self-hosted monitoring tool for tracking website and server uptime",
      "homepage": "https://github.com/louislam/uptime-kuma",
      "category": "monitoring",
      "command": "docker run -d -p 4007:3001 --restart  unless-stopped louislam/uptime-kuma:1"
    },
    {
      "name": "myspeed",
      "description": "A speed test analysis software that stores your internet speed for up to 30 days",
      "homepage": "https://github.com/gnmyt/myspeed",
      "category": "networking",
      "command": "docker run -d -p 4008:5216 -v $HOME/docklite/myspeed:/myspeed/data --restart unless-stopped germannewsmaker/myspeed"
    },
    {
      "name": "qbittorrent",
      "description": "An open-source BitTorrent client with a web interface and VPN support",
      "homepage": "https://github.com/hotio/qbittorrent",
      "category": "filesharing",
      "command": "docker run -d -p 4009:8080 -v $HOME/docklite/qbittorrent:/config -v $HOME/docklite/downloads:/downloads --restart unless-stopped ghcr.io/hotio/qbittorrent:latest"
    },
    {
      "name": "telearia",
      "description": "Telegram-Controlled Cloud Downloader for Files and Videos",
      "homepage": "https://github.com/besoeasy/telearia",
      "category": "filesharing",
      "command": "docker run -d --restart unless-stopped -p 4010:6799 -v $HOME/docklite/downloads:/tmp/telearia -e TELEGRAMBOT=Your-Telegram-Bot-Token ghcr.io/besoeasy/telearia:main"
    },
    {
      "name": "cobalt",
      "description": "A privacy-focused, self-hosted web browser proxy (Cobalt fork)",
      "homepage": "https://github.com/imputnet/cobalt",
      "category": "utilities",
      "command": "docker run -d -p 4012:8080 --restart unless-stopped ghcr.io/imputnet/cobalt:latest"
    },
    {
      "name": "duplicati",
      "description": "A free backup software to store encrypted backups online for Windows, macOS, and Linux.",
      "homepage": "https://github.com/duplicati/duplicati",
      "category": "backup",
      "command": "docker run -d -p 4013:8200 --restart unless-stopped duplicati/duplicati"
    },
    {
      "name": "memos",
      "description": "An open-source, self-hosted memo hub with knowledge management and collaboration features",
      "homepage": "https://github.com/usememos/memos",
      "category": "utilities",
      "command": "docker run -d -p 4014:5230 -v $HOME/docklite/memos:/var/opt/memos --restart unless-stopped neosmemo/memos:stable"
    },
    {
      "name": "gitea",
      "description": "A lightweight, self-hosted Git service with a web interface.",
      "homepage": "https://github.com/go-gitea/gitea",
      "category": "development",
      "command": "docker run -d -p 4015:3000 -p 4016:22 --restart unless-stopped -v $HOME/docklite/gitea:/data gitea/gitea:latest"
    },
    {
      "name": "jellyfin",
      "description": "Open-source media system for streaming movies, TV, and music.",
      "homepage": "https://github.com/jellyfin/jellyfin",
      "category": "media",
      "command": "docker run -d -p 4017:8096 -v $HOME/docklite/jellyfin:/config -v $HOME/docklite/downloads:/media --restart unless-stopped jellyfin/jellyfin"
    },
    {
      "name": "vaultwarden",
      "description": "Lightweight Bitwarden-compatible password manager.",
      "homepage": "https://github.com/dani-garcia/vaultwarden",
      "category": "utilities",
      "command": "docker run -d -p 4018:80 -v $HOME/docklite/vaultwarden:/data --restart unless-stopped vaultwarden/server:latest"
    }
  ]
}
